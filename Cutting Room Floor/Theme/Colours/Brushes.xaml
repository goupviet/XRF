<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="MainAccentBrush" Color="{StaticResource MainAccent}" />
    <SolidColorBrush x:Key="SubAccentBrush" Color="{StaticResource SubAccent}" />
    <SolidColorBrush x:Key="HighlightBrush" Color="{StaticResource Highlight}" />
</ResourceDictionary>